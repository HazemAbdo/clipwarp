{
  "expo": {
    "name": "ClipWarp",
    "slug": "expo-template-blank-typescript",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mokhincode.ClipWarp"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "eas": {},
    "plugins": [
      "expo-asset", 
      [
      "@bam.tech/react-native-app-security",
      {
        "sslPinning": {
          "192.168.1.13": [
            "rtJ2pHLlrE5hyEh3I/rdFMaP+qoObKwXm1Lv+SWCmj4="
          ]
        },
        "preventRecentScreenshots": {
          "ios": { "enabled": true },
          "android": { "enabled": true }
        }
      }
    ]
    ],
    "extra": {
      "eas": {
        "projectId": "ffea7838-506a-44fa-8d17-9ead3a93c317"
      }
    }
  }
}
