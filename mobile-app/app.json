{
  "expo": {
    "name": "ClipWarp",
    "slug": "clipwarp",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mokhincode.ClipWarp"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "eas": {},
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/noti-icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-asset"
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "ffea7838-506a-44fa-8d17-9ead3a93c317"
      }
    }
  }
}
